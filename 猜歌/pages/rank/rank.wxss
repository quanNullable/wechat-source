page {
    height: 100%;
}

.swiper-tab {
    margin: 34rpx 30rpx 22rpx 30rpx;
    height: 88rpx;
    padding: 10rpx;
    box-sizing: border-box;
    border-radius: 44rpx;
    background-color: #460166;
}

.swiper-tab .tab-list {
    display: inline-block;
    width: 50%;
    height: 68rpx;
    line-height: 68rpx;
    font-size: 28rpx;
    color: #b2a4bd;
    text-align: center;
}

.swiper-tab .tab-list.on {
    background: #f75298;
    color: #fff;
    border-radius: 40rpx;
}

.swiper-box {
    height: calc(100% - 274rpx);
}

.swiper-bottom {
    display: flex;
    align-items: center;
    justify-content: center;
    height: 130rpx;
}

.swiper-bottom text {
    display: inline-block;
    padding: 0 50rpx;
    width: auto;
    height: 72rpx;
    line-height: 72rpx;
    background: #7401aa;
    border-radius: 36rpx;
    color: #fff;
    text-align: center;
}

.rank-list {
    padding: 0 30rpx;
}

.rank-item {
    position: relative;
    margin-top: 10rpx;
    padding: 20rpx 30rpx;
    height: 80rpx;
    background: #440164;
    color: #fff;
    border-radius: 10rpx;
}

.rank-item:first-child:after {
    content: "";
    position: absolute;
    left: 70rpx;
    top: -5rpx;
    width: 72rpx;
    height: 43rpx;
    background: url(data:image/png;base64,);
    background-repeat: no-repeat;
    background-position: 0 0;
    background-size: cover;
}

.rank-item .num {
    float: left;
    line-height: 80rpx;
    text-align: center;
    font-size: 36rpx;
    color: #fff;
}

.rank-item .avatar {
    float: left;
    margin-left: 30rpx;
    width: 80rpx;
    height: 80rpx;
    border-radius: 100%;
}

.rank-item .name {
    float: left;
    margin-left: 30rpx;
    width: 290rpx;
    line-height: 80rpx;
    font-size: 28rpx;
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
}

.rank-item .gift {
    float: right;
    line-height: 80rpx;
    font-size: 28rpx;
    color: #acacac;
}

.rank-item .gift text {
    margin: 0 5rpx;
}