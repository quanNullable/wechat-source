.page {
    display: flex;
    flex-direction: column;
    background-color: #efefef;
    height: 100%;
}

.link_txt {
    color: #099FDE;
}

@font-face {
    font-family:'ctrip_icon';src:url(data:application/octet-stream;base64,%3D) format('truetype');
}

icon {
    display: inline-block;
    vertical-align: middle;
}

icon:before {
    font-size: 36rpx;
}

.ico-point_red {
    width: 8px;
    height: 8px;
    overflow: hidden;
    border-radius: 50%;
    background-color: #F43530;
}

.ico-point_text {
    float: left;
    min-width: 18px;
    height: 18px;
    white-space: nowrap;
    color: #fff;
    background-color: #F43530;
    font-size: 13px;
    text-align: center;
    line-height: 1.5;
    border-radius: 9px;
    box-sizing: border-box;
    padding: 0 6px;
}

.ico-header:before {
    font-family: 'ctrip_icon';
    content: "\EC01";
}

.ico-phone:before {
    font-family: 'ctrip_icon';
    content: "\EC02";
}

.ico-coord:before {
    font-family: 'ctrip_icon';
    content: "\EC03";
}

.ico-fav:before {
    font-family: 'ctrip_icon';
    content: "\EC04";
}

.ico-service:before {
    font-family: 'ctrip_icon';
    content: "\EC05";
}

.ico-search:before {
    font-family: 'ctrip_icon';
    content: "\EC06";
}

.ico-order:before {
    font-family: 'ctrip_icon';
    content: "\EC07";
}

.ico-message:before {
    font-family: 'ctrip_icon';
    content: "\EC08";
}

.ico-add:before {
    font-family: 'ctrip_icon';
    content: "\EC09";
}

.ico-info:before {
    font-family: 'ctrip_icon';
    content: "\EC0A";
}

.ico-edit:before {
    font-family: 'ctrip_icon';
    content: "\EC0B";
}

.ico-goback:before {
    font-family: 'ctrip_icon';
    content: "\EC0C";
}

.ico-go:before {
    font-family: 'ctrip_icon';
    content: "\EC0D";
}

.ico-solgan:before {
    font-family: 'ctrip_icon';
    content: "\EC0E";
    opacity: 0.2;
}

page {
    font-size: 32rpx;
    color: #333;
    background-color: #EFEFEF;
    font-family: -apple-system-font,Helvetica Neue,Helvetica,sans-serif;
}

.color-interact {
    color: #099FDE!important;
}

.color-money {
    color: #FF7D13!important;
}

.color-warn {
    color: #F43530!important;
}

.color-gray_333 {
    color: #333!important;
}

.color-gray_666 {
    color: #666!important;
}

.color-gray_999 {
    color: #999!important;
}

.color-gray_bbb {
    color: #bbb!important;
}

.color-bg {
    background-color: #EFEFEF;
}

.color-bg_fff {
    background-color: #fff;
}

.color-note {
    color: #fe6b5b!important;
}

.btn-primary {
    color: #fff;
    background-color: #5495e6;
}

.btn-primary_hover {
    color: rgba(255,255,255,0.6);
    background-color: #5495e6;
}

.btn-default {
    color: #fff;
    background-color: #52BCE8;
}

.btn-default_hover {
    color: rgba(255,255,255,0.6);
    background-color: #5495e6;
}

.btn-minor {
    color: #099FDE;
    background-color: #fff;
}

.btn-minor_hover {
    color: rgba(9,159,222,0.6);
    background-color: #dedede;
}

.btn-line {
    color: #5495e6;
    background-color: transparent;
    border: 0 none;
    box-shadow: 1px 1px 0 #5495e6 inset,-1px -1px 0 #5495e6 inset;
}

.btn-line-light {
    color: #fc6e51;
    background-color: transparent;
    border: 1px solid #e0e0e0;
}

.btn-line-ligh::after {
    border: 0;
}

.btn-line_hover {
    background-color: #dedede;
}

.btn-disabled {
    color: #e6e6e6;
    background-color: #ccc;
}

.tips_top {
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    text-align: center;
    font-size: 15px;
    line-height: 1.5;
    padding: 5px 10px;
    color: #fff;
    background-color: #111;
    opacity: 0.7;
    z-index: 99;
}

.failure-page {
    position: relative;
    padding-top: 115px;
    margin-top: 20%;
}

.failure-page_icon {
    position: absolute;
    top: 0;
    left: 50%;
    display: block;
    transform: translate(-50%,0);
}

.failure-page_icon:after {
    content: "";
    position: absolute;
    top: 50%;
    left: 50%;
    width: 60%;
    height: 60%;
    transform: translate(-50%,-50%);
    background-color: #fff;
    z-index: -1;
}

.failure-page_word {
    padding-bottom: 40px;
}

.failure-page_text {
    display: block;
    text-align: center;
    font-size: 16px;
    color: #666;
    line-height: 1.6;
}

.failure-page_btn {
    margin: 0 20px 20px;
}

.copyright {
    position: fixed;
    bottom: 10px;
    left: 10px;
    right: 10px;
    text-align: center;
    font-size: 10px;
    line-height: 1.8;
    color: #B2B2B2;
}

.foot_link {
    display: block;
    font-size: 13px;
}

.tab-module {
    position: relative;
    display: flex;
    width: 100%;
    height: 94rpx;
    border-bottom: 1px solid #E5E5E5;
    background-color: #FAFAFA;
    color: #333;
    z-index: 90;
}

.tab-item {
    flex: 1 1 auto;
    height: 94rpx;
    padding-top: 14rpx;
    line-height: 70rpx;
    box-sizing: border-box;
}

.tab-item_current {
    color: #099FDE!important;
    border-bottom: 3px solid #099FDE!important;
}

.tab-text {
    display: block;
    text-align: center;
    font-size: 30rpx;
}

.search-module {
    position: relative;
    display: block;
    background-color: #EFEFEF;
    z-index: 99;
}

.search-module_ico {
    position: absolute;
    top: 17rpx;
    left: 50%;
    width: 25rpx;
    height: 25rpx;
    transform: translateX(-120%);
}

.search-module_ico:before {
    position: absolute;
    top: 0;
    left: 0;
    color: #B2B2B2;
    font-size: 25rpx;
    line-height: 1;
    overflow: hidden;
}

.search-module_ico_focus {
    left: 24rpx;
    transform: translateX(0);
}

.search-module_input {
    height: 56rpx;
    font-size: 28rpx;
    line-height: 60rpx;
    background-color: #fff;
    box-sizing: border-box;
    border-radius: 5px;
    padding-left: 50rpx;
    margin: 16rpx;
}

.search-module_input_focus {
    font-size: 26rpx;
    color: #B2B2B2;
    padding-left: 50%;
    text-indent: -1em;
}

.search-module_input-f {
    margin-right: 100rpx;
    border-right: 60rpx solid #fff;
}

.search-module_clear {
    position: absolute;
    top: 14rpx;
    right: 120rpx;
}

.search-module_cancel {
    position: absolute;
    top: 14rpx;
    right: 16rpx;
    font-size: 32rpx;
    color: #099FDE;
}

.search-list_arrow:after {
    content: "";
    position: absolute;
    top: 36rpx;
    right: 30rpx;
    width: 16rpx;
    height: 16rpx;
    overflow: hidden;
    font-size: 0;
    border-right: 4rpx solid #C7C7C7;
    border-bottom: 4rpx solid #C7C7C7;
    transform: rotate(-45deg);
}

.search-list {
    position: relative;
    display: block;
    overflow: hidden;
    margin-bottom: 20rpx;
    padding-left: 32rpx;
    color: #666;
    background-color: #fff;
    border-top: 1px solid #E5E5E5;
    border-bottom: 1px solid #E5E5E5;
}

.search-list_item {
    height: 88rpx;
    margin-top: -1px;
    font-size: 28rpx;
    line-height: 98rpx;
    text-indent: 34rpx;
    border-top: 1px solid #E5E5E5;
}

.search-list_txt {
    color: #099FDE;
}

.qp-share-tips {
    position: fixed;
    left: 0;
    top: 0;
    right: 0;
    bottom: 0;
    background: rgba(0,0,0,.75);
    z-index: 1000;
}

.qp-share-tips .qp-share-txt {
    color: #fff;
    margin: 40rpx 40rpx 20rpx 30%;
    font-size: 30rpx;
    line-height: 50rpx;
}

.qp-share-tips .btn-know {
    background: transparent;
    border: 1px solid #fff;
    color: #fff;
    margin-top: 40rpx;
    width: 320rpx;
    height: 80rpx;
    line-height: 80rpx;
}

.qp-share-tips .btn-know_hover::after {
    background: rgba(0,0,0,.1);
}

.color-primary {
    color: #5495e6;
}

.color-orange {
    color: #ff9a14;
}

.bdcolor-primary {
    border-color: #5495e6;
}

.bgcolor-primary {
    background-color: #5495e6;
}

.shade-box {
    position: fixed;
    top: 0;
    right: 0;
    bottom: 0;
    left: 0;
    background: rgba(0,0,0,.7);
    z-index: 990;
}